<div class="function-headline">
	<h1>Quản trị viên</h1>
</div>

<table class="table table-bordered">
	<thead>
		<tr>
			<th>Họ và tên</th>
			<th>Tên tài khoản</th>
			<th>Ngày đăng kí</th>
			<th>Chức năng</th>
		</tr>
	</thead>
	<tbody>
		<% @moderators.each do |moderator| %>
		<tr>
			<td><%= moderator.fullname %></td>
			<td><%= moderator.username %></td>
			<td><%= time_ago_in_words(moderator.created_at) %> ago</td>
			<td><%= link_to 'Sửa', edit_admin_moderator_path(moderator) %></td>
		</tr>
		<% end %>
	</tbody>
</table>